{
    "extends": ["airbnb", "react-app"],
    "rules": {
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
        "jsx-a11y/anchor-is-valid": [ "error", { "components": [ "Link" ], "specialLink": [ "to" ] } ],
        "no-underscore-dangle": 0
    },
    "globals": {
        "document": true,
        "localStorage": true
    }
}